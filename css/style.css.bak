/* zuopin */
.zuopin {
	padding: 2% 0;

	background-size: 100% 100%; 
}
#zuopin .wrap{padding-bottom: 2%;}
.zuopin h3{
	color: #000;
	font-size: 2.5em;
	font-weight: 700;
	text-align: center;
	padding: 2%;
}
#zuopin .wrap h4{
	color: #000;
	font-size: 1em;
	line-height:1.2em;
	font-weight: 500;
	padding:2% 0 2% 2%;
}

.zuopin p.a{
	font-family: "MicrosoftYaHei";
	font-size: 0.8em;
	padding: 0.5em 0;
	color: #777777;
	line-height: 1.5em;
	
	text-align:center;
	margin-bottom: 2%;
	
}
.zuopin .wrap p{display:table; width:100%; text-align:center;}
.zuopin .wrap a {
	
	width:30.6%;
	text-align: center;
	margin-top: 1%;
	background: #fff;
	padding: 1%;
	display:table-cell;
	box-shadow:0px 0px 10px #999999;
	padding: 2% 0;
}

.zuopin .wrap a:hover{ box-shadow:inset 0px 0px 10px #999999; border-collapse:separate;}
/*---start-bottom-footer---*/
.bottom-footer{clear:both;}
.bottom-footer wrap{
	margin:0 auto; width:70%; text-align: center;
}
.bottom-footer p{
	font-size: 0.875em;
	color: #777;
	margin-top: 1.5em;
	text-align:center;
}
